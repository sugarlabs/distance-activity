#: activity/activity.info:2
msgid "Distance"
msgstr "Bor piny"

#: activity.py:162
msgid "Atmosphere"
msgstr "Kan oguruwa"

#: activity.py:165
msgid "Custom metric"
msgstr "Metrik ame otio kede"

#: activity.py:187
msgid ""
"To measure the distance between two laptops, you must first share this "
"Activity."
msgstr "Me miyi pimo yi akina laptop aryo, cak ki tiyo Tic man."

#: activity.py:189
msgid "Press the button to measure the distance to another laptop"
msgstr "Di mapeca me wek ipim bor piny itunu i laptop mukene"

#: activity.py:191
msgid "Preparing to measure distance"
msgstr "Tye kayike me pimo bor piny"

#: activity.py:192
msgid "Ready to make a measurement.  Waiting for partner to be ready."
msgstr "Otyeko yike oko mecako pimo. Tye kakuru ngat ma gitiyo kwede me yike."

#: activity.py:194
msgid "Recording sound from each laptop."
msgstr "Mako dwon maya i laptop acelacel."

#: activity.py:195
msgid "Processing recorded audio."
msgstr "Tye kayiko dwon ma gimako."

#: activity.py:197
msgid "This activity already has two participants, so you cannot join."
msgstr "Tic man dong tye jo matiyo aryo, mumio yin pe doki iromo donyo iye."

#: activity.py:200
msgid "Begin Measuring Distance"
msgstr "Cak Pimo Bor Piny"

#: activity.py:201
msgid "Stop Measuring Distance"
msgstr "Gik. Pimo Bor Piny"

#: activity.py:235 smoot_toolbar.py:99
#, python-format
msgid "Measured distance in %s"
msgstr "Bor piny otyeko pimo i %s"

#: activity.py:235 smoot_toolbar.py:30 smoot_toolbar.py:34 smoot_toolbar.py:77
#: smoot_toolbar.py:100
msgid "meters"
msgstr "mita"

#: atm_toolbars.py:65
msgid "Temperature (C): "
msgstr "Lyeto (C): "

#: atm_toolbars.py:70
msgid "Relative Humidity (%): "
msgstr "Kite me yamo (%): "

#: atm_toolbars.py:75
msgid "Speed of Sound (m/s): "
msgstr "Dwiro akop iyamo (m/s): "

#: calibration_toolbar.py:28
msgid "Calibration Offset (meters): "
msgstr "Yik me pimo okeketo oko (mita): "

#: smoot_toolbar.py:30 smoot_toolbar.py:35
msgid "centimeters"
msgstr "centimita"

